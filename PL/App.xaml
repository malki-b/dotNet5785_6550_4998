<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <local:VolunteerCollection x:Key="VolunteerCollectionKey" />
        <local:RolesCollection x:Key="RolesCollectionKey" />
        <local:DistanceCollection x:Key="DistanceCollectionKey" />
        <local:CallFieldCollection x:Key="CallFieldCollectionKey" />
        <local:TypeOfReadingCollection x:Key="TypeOfReadingCollectionKey" />
        <local:ClosedCallFieldCollection x:Key="ClosedCallFieldCollectionKey" /> 
        <local:OpenCallFieldCollection x:Key="OpenCallFieldCollectionKey" />
        <local:NullToVisibilityConverter x:Key="NullToVisibilityConverterKey" />
        <local:NullToBoolConverter x:Key="NullToBoolConverterKey" />
        <local:VolunteerStatusConverter x:Key="VolunteerStatusConverterKey"/>

    </Application.Resources>
</Application>
